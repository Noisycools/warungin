  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "laravel-mix": "^6.0.43",
    "postcss": "^8.4.5",
    "tailwind-watch": "0.0.6",
    "tailwindcss": "^3.0.13"
  },
